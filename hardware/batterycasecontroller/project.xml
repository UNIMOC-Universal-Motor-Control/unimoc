<library>
  <repositories>
    <repository><path>../../../modm/repo.lb</path></repository>
  </repositories>
  <options>
    <option name="modm:build:project.name">unimoc_battery_case_controller</option>
    <option name="modm:target">stm32g473cbt6</option>
    <option name="modm:build:build.path">build</option>
    <option name="modm:build:cmake:include_cmakelists">false</option>
    <option name="modm:build:make:include_makefile">false</option>
    <option name="modm:platform:cortex-m:linkerscript.override">hardware/batterycasecontroller/modm/link/linkerscript.ld</option>
    <option name="modm:platform:rtt:buffer.rx">512</option>
    <option name="modm:platform:rtt:buffer.tx">512</option>
  </options>

  <modules>
    <module>modm:platform:core</module>
    <module>modm:platform:gpio</module>
    <module>modm:platform:adc:1</module>
    <module>modm:platform:adc:2</module>
    <module>modm:platform:adc:3</module>
    <module>modm:platform:adc:4</module>
    <module>modm:platform:adc:5</module>
    <module>modm:platform:timer:8</module>
    <module>modm:platform:rtt</module>
    <module>modm:build:cmake</module>
    <module>modm:debug</module>
  </modules>
</library>